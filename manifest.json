{
  "manifest_version": 2,
  "name": "Niki",
  "minimum_chrome_version": "24.0.1307.0",
  "version": "1.3",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
     "optional_permissions": [
    "serial"
  ],
  "permissions": [
    "http://*/",
    "https://*/",
    "webview",
    "audioCapture"
  ]
}
